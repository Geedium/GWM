  pointer-events: none;
}
aside nav ul li div button .dropdown-caret i {
  margin-right: 0;
  line-height: inherit;
}
aside nav ul li div button i {
  text-anchor: middle !important;
  text-align: center !important;
}
aside nav ul li div button i:not(.drop) {
  position: relative;
  left: 30px;
  width: 35px;
  margin-right: 40px;
  text-align: left;
