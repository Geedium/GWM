  overflow-anchor: none;
  color: var(--text-color);
  font-family: "Open Sans", sans-serif;
  scrollbar-color: var(--scrollbar-color-a) var(--scrollbar-color-b);
  scrollbar-width: thin;
}
@media only screen and (max-width: 667px) {
  body, html {
    overflow-x: hidden;
  }
}

